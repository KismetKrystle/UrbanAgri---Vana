{
  {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "overrides":
      [
        {
          "files": "*.sol",
          "options":
            {
              "printWidth": 120,
              "tabWidth": 4,
              "useTabs": false,
              "singleQuote": false,
              "explicitTypes": "always",
            },
        },
      ],
  },
}
