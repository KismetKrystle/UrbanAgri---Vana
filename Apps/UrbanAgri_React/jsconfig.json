{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "*": ["src/*"]
    },
    "typeRoots": ["./node_modules/@types"], 
    "skipLibCheck": true 
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}
